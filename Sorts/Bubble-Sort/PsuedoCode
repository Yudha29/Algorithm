Bubble Sort

Declare
I, j, k, temp : Integer
IsSorted : Boolean
A[0, 5] : Integer

Description
FOR I <- 0 to length[a] DO
	IsSorted <- true
	FOR j <- 0 to (length[a] - 1) – I DO
		IF (a[ j ] > a[ j + 1]) THEN
			Temp <- a[ j ]
			A[ j ] <- a[ j  + 1 ]
			A[ J + 1 ] <- temp
			IsSorted <- false
		ENDIF
	ENDFOR
	IF (isSorted) THEN
		BREAK;
	ENDIF
ENDFOR
FOR k <- 0 to length(a) DO
	WRITE(a[ k ])
ENDFOR

